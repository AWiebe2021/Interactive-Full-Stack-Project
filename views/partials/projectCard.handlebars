{{!-- <template id="card-template"> --}}
  <!-- JavaScript Bundle with Popper -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-/bQdsTh/da6pkI1MST/rWKFNjaCP5gBSY4sEBT38Q/9RBh9AH40zEOg7Hlq2THRZ" crossorigin="anonymous"></script>
 {{!-- {{#items}} --}}
  <div class="card">
    {{!-- <img class="card-img-top" src="https://via.placeholder.com/200x100" alt="Card image cap"> --}}
    <div class="card-body">
      <h5 class="card-title">{{title}}</h5>
      {{!-- <p class="card-text">{{shipping_address}}</p> --}}
      <p class="card-text">{{shipping_city}}</p>
      <p class="card-text">{{shipping_state}}</p>
      <p id="pro_id" class="card-text" style="visibility: hidden;">{{process_id}}</p>
      <p id="project_id" class="card-text" style="visibility: hidden;">{{id}}</p>
      <p class="card-text">{{customer.customer_name}}</p>
      {{!-- <p class="card-text">{{process_id}}</p> --}}
    </div>
    <div class="card-footer text-muted">
      <a id="advProject-btn" class="btn btn-outline-secondary float-right">Advance Project ></a>
      {{!-- <a href="/dashboard/edit/{{project.id}}" class="edit-link">Edit project</a> --}}
      <a class="btn btn-outline-secondary float-left" data-bs-toggle="modal" data-bs-target="#myModal{{@index}}">read more</a>
    </div>
  </div>
  
  <!-- Modal -->
<div class="modal fade" id="myModal{{@index}}" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-dialog-scrollable" role="document">
    <div class="modal-content">
      <div class="modal-header bg-light">
        <h5 class="card-title" id="exampleModalLabel">{{title}}</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
            {{!-- <img class="card-img-top" src="{{image}}" alt="Card image cap"> --}}
            <p class="card-text">{{{customer.customer_name}}}</p>
            <p class="card-text">{{{shipping_address}}}</p>
            <p class="card-text">{{{shipping_city}}}</p>
            <p class="card-text">{{{shipping_state}}}</p>
            <p class="card-text">{{{shipping_zip}}}</p>
      </div>
      <div class="modal-footer bg-light">
        <button type="button" class="btn  btn-outline-secondary" data-bs-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
  {{!-- {{/items}} --}}
{{!-- </template> --}}

<div id="content"  class="card-grid"></div>
{{!-- <script src="/public/javascript/projectCard.js"></script> --}}
<script src="/javascript/advProject.js"></script>
